# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
#[*]
#end_of_line = lf
#insert_final_newline = true

# 4 tab indentation
[*.cpp]
indent_style = space
indent_size = 4

[*.h]
indent_style = space
indent_size = 4

[*.txt]
indent_style = space
indent_size = 4
